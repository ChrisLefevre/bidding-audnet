{
  "name": "Bidding Extension",
  "description": "Test Bidding Extension",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Bidding Extension"
  },
  "icons": {
    "19": "favicon.png"
  },
  "manifest_version": 2,
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentscript.js"]
      }
    ],
    "web_accessible_resources": ["script.js"]
}
